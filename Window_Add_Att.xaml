        <Label x:Name="label_Copy2" Content="Index:" HorizontalAlignment="Left" Margin="26,180,0,0" VerticalAlignment="Top" Width="132"/>
        <TextBox x:Name="textBox_Copy3" HorizontalAlignment="Left" Height="23" Margin="163,248,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="254"/>
