        <ComboBox x:Name="comboBox_Att_Type_Get" HorizontalAlignment="Left" Margin="163,292,0,0" VerticalAlignment="Top" Width="254"/>
        <Label x:Name="label_Copy6" Content="Tag:" HorizontalAlignment="Left" Margin="26,16,0,0" VerticalAlignment="Top" Width="132"/>
        <ComboBox x:Name="comboBox_Tag" HorizontalAlignment="Left" Margin="163,20,0,0" VerticalAlignment="Top" Width="254" SelectionChanged="ComboBox_Copy3_SelectionChanged"/>
